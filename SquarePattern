
import java.util.Scanner;
public class SquarePattern {
	public static void main(String[]args) {
		int n,i=1;
		Scanner s = new Scanner(System.in);
		n= s.nextInt();

		while(i<=n) {
			int j = 1, p=i;
			
			while(j<=i) {
				System.out.print(p);
				p++;
				j++;
				
			}
			System.out.println("\n");
			i++;
		}
		
		
	}
	
	}
