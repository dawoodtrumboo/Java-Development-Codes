import java.util.*;
public class TermsofAP {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int num=0;		
			int x=1;
			for(;num<=4*n+1;x++) {
//				System.out.print("12");
				num = 3*x+2;
			if(num%4!=0) {
//				System.out.print("15");
				System.out.print(num + " ");
			}
			
			else
			{
//				System.out.print("21");
				continue;
			}
				
				
			
		}
		

	}
}
