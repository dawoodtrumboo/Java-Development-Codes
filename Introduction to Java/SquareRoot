import java.util.*;
public class SquareRoot {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		for(int i =0;i*i<=n;i++) {
			if(i*i>=n-2*i&&i*i<=n) {
				System.out.println("Square root of " + n + " is " + i);
			}
			else 
				continue;
		}

	}

}
