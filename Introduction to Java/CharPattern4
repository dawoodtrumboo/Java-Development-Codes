import java.util.Scanner;
public class CharPattern4 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int i = 1;
        while(i<=n){
        	int j = 1;
            char p = 65;
            p= (char)(p + i - 1);
            while(j<=i){
            	System.out.print(p);
            	p++;
				j++;
            }
            System.out.println();
			i++;
        }
	
		
	}
}
