import java.util.Scanner;
public class NthFibonacciNumber {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int a=0,b=1,c=0,N;
		if(n==1) {
			System.out.print(1);
		}else {
			System.out.print("Fibonacci Series : ");
		for(int i=2;i<=n;i++) {
			c= a+b;
			a=b;
			b=c;
			N=c;
			System.out.print(c + " ");
			if(i==n) {
				System.out.println();
				System.out.print("Nth Term of Fibonacci Series: "+ N);
			}
			
			
		}
		}

	}

}
