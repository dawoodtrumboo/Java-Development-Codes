import java.util.Scanner;
public class OddSquarre {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int i = 1;
		int num =1;
		while(i<=n) {
			int p = num;
			int j = 1;
			while(j<=n) {
				System.out.print(p);
				p+=2;
				if(i+j==n+1) {
					p=1;
				}
				j++;
			}
			num+=2;
			System.out.println();
			i++;
			}
		
		}
	}
