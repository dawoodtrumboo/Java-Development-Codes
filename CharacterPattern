import java.util.Scanner;
public class CharacterPattern {
	public static void main(String[]args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
//		String str = s.next();
//		ch = str.charAt(0);
		
		int i = 1;
		while(i<=n) {
			int j = 1;
			char p = 65;
			while(j<=n) {
				System.out.print(p);
				p++;
				j++;
			}
			System.out.println();
			i++;
		}
	}

}
